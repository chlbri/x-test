{
  "name": "@bemedev/x-test",
  "version": "0.1.1-beta",
  "description": "Test XState Machine sequentially",
  "author": {
    "email": "bri_lvi@icloud.com",
    "name": "chlbri",
    "url": "https://bemedev.vercel.app"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "module": "lib/index.mjs",
  "keywords": [
    "XState",
    "test",
    "assertion",
    "State machine",
    "Finite state",
    "StateCharts",
    "TDD"
  ],
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "config": "pnpm install",
    "rollup": "rollup -c",
    "prepare": "husky install",
    "updateDependencies": "pnpm update --latest",
    "build": "rm -rf lib && pnpm run rollup",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest --passWithNoTests",
    "remove": "rm -rf lib node_modules pnpm-lock.yaml",
    "clean": "pnpm run remove && pnpm run config",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "eslint": "eslint \"{src/**/*,App}.{ts,tsx}\"",
    "lint": "pnpm run prettier && pnpm run eslint --fix",
    "lint-watch": "onchange --await-write-finish 5500 \"**/*\" -- pnpm run lint",
    "postpublish": "git fetch & git pull",
    "prepublishOnly": "pnpm run build && pnpm run lint"
  },
  "devDependencies": {
    "@types/deepmerge": "^2.2.0",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "@vitejs/plugin-react": "^2.2.0",
    "@xstate/immer": "^0.3.1",
    "esbuild": "^0.15.14",
    "eslint": "^8.28.0",
    "husky": "^8.0.2",
    "immer": "^9.0.16",
    "onchange": "^7.1.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "rollup": "^3.3.0",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vite-tsconfig-paths": "^3.5.2",
    "vitest": "^0.25.2",
    "xstate": "^4.34.0",
    "zod": "^3.19.1"
  },
  "dependencies": {
    "@bemedev/x-matches": "^0.0.3",
    "deepmerge": "^4.2.2",
    "dequal": "^2.0.3"
  },
  "peerDependencies": {
    "xstate": "^4.30.6"
  },
  "peerDependenciesMeta": {
    "xstate": {
      "optional": true
    }
  }
}
